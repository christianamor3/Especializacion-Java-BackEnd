package n3exercici1;

public class Generica <T extends Telefon, E extends Smartphone>{

	T tipoGenericoLimitTelefon;
	E tipoGenericoLimitSmartphone;
	
	public static <T> T returnT(T tipoGenericoLimitTelefon) {
		return tipoGenericoLimitTelefon;
		
	}
	
	public static <E> E returnE(E tipoGenericoLimitSmartphone) {
		return tipoGenericoLimitSmartphone;
		
	}
	
}
