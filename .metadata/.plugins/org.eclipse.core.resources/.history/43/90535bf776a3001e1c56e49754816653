package n1exercici2;

public class CalculoDNI {

	public char calculoLetraDNI(int numeroDNI) {
		char letraDNI;
		char[] posiblesLetras = {'A', 'B', 'C', 'D', 'E', 
				'F', 'G', 'H', 'I', 'J', 'K', 'L'};
		
		if (numeroDNI<1000000) {
			letraDNI='A';
		} else if (numeroDNI<2000000) {
			letraDNI='B';
		} else if (numeroDNI<3000000) {
			letraDNI='C';
		} else if (numeroDNI<4000000) {
			letraDNI='D';
		} else if (numeroDNI<5000000) {
			letraDNI='E';
		} else if (numeroDNI<6000000) {
			letraDNI='F';
		} else if (numeroDNI<7000000) {
			letraDNI='G';
		} else if (numeroDNI<8000000) {
			letraDNI='H';
		} else if (numeroDNI<9000000) {
			letraDNI='I';
		}
			
			return letraDNI;
	}
		
	}
	
}
